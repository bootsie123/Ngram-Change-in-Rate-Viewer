{
  "name": "ngram-change-in-rate-viewer",
  "version": "1.0.0",
  "private": true,
  "description": "A simple viewer for Google's Ngram that allows you to view the change in rate of word usage over time",
  "author": "bootsie123",
  "scripts": {
    "build": "npm run vue:build -- --modern",
    "lint": "vue-cli-service lint",
    "dev": "nodemon server.js",
    "start": "node server.js",
    "vue:build": "vue-cli-service build",
    "vue:serve": "vue-cli-service serve"
  },
  "main": "main.js",
  "dependencies": {
    "@sipec/vue3-tags-input": "^3.0.4",
    "axios": "^0.21.4",
    "bulma": "^0.9.4",
    "cheerio": "^1.0.0-rc.11",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.22.7",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "node-fetch": "^2.6.7",
    "remixicon": "^2.5.0",
    "vue": "^3.2.36",
    "vue-fusioncharts": "^3.2.0",
    "vue-mathjax-next": "0.0.4",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.0.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": true
  },
  "browserslist": [
    "defaults",
    "last 2 versions",
    "not dead"
  ]
}
